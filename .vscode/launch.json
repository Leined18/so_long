{
	"version": "0.2.0",
	"configurations": [
	  {
		"name": "C/C++ Runner: Debug Session",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/so_long",
		"args": ["assets/maps/map1.ber"],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "gdb",
		"miDebuggerPath": "/usr/bin/gdb",
		"setupCommands": [
		  {
			"description": "Enable pretty-printing for gdb",
			"text": "-enable-pretty-printing",
			"ignoreFailures": true
		  }
		],
		"preLaunchTask": "build"
	  }
	]
  }
  